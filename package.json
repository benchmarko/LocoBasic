{
  "name": "locobasic",
  "version": "0.4.30",
  "description": "# LocoBasic - Loco BASIC",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint src tests",
    "clean": "rm -rf dist/src/* && rm dist/*.js && rm dist/*.js.map && rm dist/*.ts && rm dist/*.ts.map",
    "build": "tsc --build && npx rollup -c && node updateVersion.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --coverage.reportsDirectory=./tmp/coverage",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.15",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.39.1",
    "gh-pages": "^6.2.0",
    "rollup": "^4.25.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.47.0",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "ohm-js": "^17.1.0"
  }
}
